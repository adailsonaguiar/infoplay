;\n      try {\n        const res = await axios.get(\n          \"https://api.streamingmoviesright.com/site/movies\"\n        );\n        res && setHighlights(mapMoviePattern(res.data.data.data));\n      } catch (error) {\n        console.error(error.response);\n      }\n      setLoading(false);\n    }\n    !movies.highlights.length && getData();\n  }, []);\n  return (\n    <>\n      <Heading lineLeft lineColor=\"secondary\">\n        Destaques\n      </Heading>\n      {loading ? (\n        <S.MovieList>\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n          <S.CardShimmer />\n        </S.MovieList>\n      ) : (\n        <S.MovieList>\n          {movies.highlights.map((movie, index) => (\n            <Link to={`/movie/${movie.imdbID}`} key={index}>\n              <CardMovie movie={movie} />\n            </Link>\n          ))}\n        </S.MovieList>\n      )}\n    </>\n  );\n}\n",["109","110"],"/home/adailsonaguiar/dev/infoplay/src/pages/Home/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/CardMovie/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/CardMovie/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Navbar/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/routes.js",[],"/home/adailsonaguiar/dev/infoplay/src/pages/DetailMovie/index.jsx",["111"],"import { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport Heading from \"../../components/Heading\";\nimport api from \"../../config/config\";\n\nimport * as S from \"./styles\";\n\nexport default function DetailMovie() {\n  const API_KEY = process.env.REACT_APP_KEY;\n  const { id } = useParams();\n  const [movie, setMovie] = useState({ loading: true });\n\n  async function getMovie() {\n    const res = await api.get(`?apikey=${API_KEY}&i=${id}&plot=full`);\n    if (res) setMovie({ ...res.data, loading: false });\n    else setMovie({ ...movie, loading: false });\n  }\n\n  function getYouTubeId(youtubeURL) {\n    return youtubeURL.replace(\n      /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/,\n      \"$7\"\n    );\n  }\n\n  useEffect(() => {\n    getMovie();\n  }, []);\n\n  return (\n    <S.Container>\n      {movie.loading ? (\n        <>\n          <S.PostShimmer />\n          <S.InfoTextShimmer />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer />\n        </>\n      ) : (\n        <>\n          <S.Header>\n            <S.Poster src={movie.Poster} />\n            <Heading>{movie.Title}</Heading>\n          </S.Header>\n          <S.InfoWrapper>\n            <S.SpaceWrapper></S.SpaceWrapper>\n            <S.InfoText>{movie.Genre}</S.InfoText>\n            <S.SpaceWrapper>\n              <S.RowWrapper>\n                <S.InfoText>{movie.Runtime}</S.InfoText>\n                <S.ImdbIcon src=\"https://assets.website-files.com/5cd4713f7f96b4b83247d01d/5cd4b1e190d2dee152a71076_imdb.svg\" />\n                <S.InfoText>{movie.imdbRating}</S.InfoText>\n              </S.RowWrapper>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>{movie.Plot}</S.InfoText>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>Director: {movie.Director}</S.InfoText>\n              <S.InfoText>Release: {movie.Released}</S.InfoText>\n              <S.InfoText>Actors: {movie.Actors}</S.InfoText>\n            </S.SpaceWrapper>\n            {movie.trailer && (\n              <S.SpaceWrapper>\n                <S.ResponsiveIframe\n                  title=\"Titulo do Iframe\"\n                  src={`https://www.youtube.com/embed/${getYouTubeId(\n                    movie.trailer\n                  )}?autoplay=0&mute=1`}\n                  frameBorder=\"0\"\n                  allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\"\n                  allowFullScreen\n                />\n              </S.SpaceWrapper>\n            )}\n          </S.InfoWrapper>\n        </>\n      )}\n    </S.Container>\n  );\n}\n","/home/adailsonaguiar/dev/infoplay/src/pages/DetailMovie/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Heading/styles.js",["112"],"import styled, { css } from \"styled-components\";\nimport media from \"styled-media-query\";\n\nconst wrapperModifiers = {\n  small: (theme) => css`\n    font-size: ${theme.font.sizes.medium};\n    &::after {\n      width: 3rem;\n    }\n  `,\n\n  medium: (theme) => css`\n    font-size: ${theme.font.sizes.xlarge};\n  `,\n\n  huge: (theme) => css`\n    font-size: ${theme.font.sizes.huge};\n  `,\n\n  lineLeft: (theme, lineColor) => css`\n    padding-left: ${theme.spacings.xxsmall};\n    border-left: 0.7rem solid ${theme.colors[lineColor]};\n  `,\n\n  lineBottom: (theme, lineColor) => css`\n    position: relative;\n    margin-bottom: ${theme.spacings.medium};\n    &::after {\n      position: absolute;\n      left: 0;\n      bottom: -0.5rem;\n      content: \"\";\n      width: 5rem;\n      border-bottom: 0.5rem solid ${theme.colors[lineColor]};\n    }\n  `,\n};\nexport const Wrapper = styled.h2`\n  ${({ theme, color, lineLeft, lineBottom, lineColor, size }) => css`\n    color: ${theme.colors[color]};\n    ${lineLeft && wrapperModifiers.lineLeft(theme, lineColor)}\n    ${lineBottom && wrapperModifiers.lineBottom(theme, lineColor)}\n    ${!!size && wrapperModifiers[size](theme)}\n  `}\n`;\n","/home/adailsonaguiar/dev/infoplay/src/components/Heading/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/pages/MainRoute.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/Footer/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/TextField/styles.js",[]