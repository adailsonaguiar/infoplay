  return youtubeURL.replace(\n      /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/,\n      \"$7\"\n    );\n  }\n\n  useEffect(() => {\n    getMovie();\n  }, []);\n\n  return (\n    <S.Container>\n      {movie.loading ? (\n        <>\n          <S.PostShimmer />\n          <S.InfoTextShimmer />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer2 />\n          <S.InfoTextShimmer />\n        </>\n      ) : (\n        <>\n          <S.Header>\n            <S.Poster src={movie.Poster} />\n            <Heading>{movie.Title}</Heading>\n          </S.Header>\n          <S.InfoWrapper>\n            <S.SpaceWrapper></S.SpaceWrapper>\n            <S.InfoText>{movie.Genre}</S.InfoText>\n            <S.SpaceWrapper>\n              <S.RowWrapper>\n                <S.InfoText>{movie.Runtime}</S.InfoText>\n                <S.ImdbIcon src=\"https://assets.website-files.com/5cd4713f7f96b4b83247d01d/5cd4b1e190d2dee152a71076_imdb.svg\" />\n                <S.InfoText>{movie.imdbRating}</S.InfoText>\n              </S.RowWrapper>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>{movie.Plot}</S.InfoText>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>Director: {movie.Director}</S.InfoText>\n              <S.InfoText>Release: {movie.Released}</S.InfoText>\n              <S.InfoText>Actors: {movie.Actors}</S.InfoText>\n            </S.SpaceWrapper>\n            {movie.trailer && (\n              <S.SpaceWrapper>\n                <S.ResponsiveIframe\n                  title=\"Titulo do Iframe\"\n                  src={`https://www.youtube.com/embed/${getYouTubeId(\n                    movie.trailer\n                  )}?autoplay=0&mute=1`}\n                  frameBorder=\"0\"\n                  allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\"\n                  allowFullScreen\n                />\n              </S.SpaceWrapper>\n            )}\n          </S.InfoWrapper>\n        </>\n      )}\n    </S.Container>\n  );\n}\n","/home/adailsonaguiar/dev/infoplay/src/pages/DetailMovie/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Heading/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Heading/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/pages/MainRoute.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/Footer/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/TextField/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Shimmer/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/Navbar/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/contexts/index.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Button/index.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/TextField/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/config/config.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Footer/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/pages/MovieList/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/pages/MovieList/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/styles/theme.js",["110"],"export default {\n  grid: {\n    container: \"130rem\",\n    gutter: \"3.2rem\",\n  },\n  border: {\n    radius: \"0.4rem\",\n  },\n  font: {\n    family:\n      \"Poppins, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif\",\n    light: 300,\n    normal: 400,\n    bold: 600,\n    sizes: {\n      xsmall: \"1.2rem\",\n      small: \"1.4rem\",\n      medium: \"1.6rem\",\n      large: \"1.8rem\",\n      xlarge: \"2.0rem\",\n      xxlarge: \"2.8rem\",\n      huge: \"5rem\",\n    },\n  },\n  colors: {\n    primary: \"#F231A5\",\n    secondary: \"#3CD3C1\",\n    mainBg: \"#06092B\",\n    lightBg: \"#F2F2F2\",\n    white: \"#FAFAFA\",\n    black: \"#0305