S.InfoText>{movie.Runtime}</S.InfoText>\n                <S.ImdbIcon src=\"https://assets.website-files.com/5cd4713f7f96b4b83247d01d/5cd4b1e190d2dee152a71076_imdb.svg\" />\n                <S.InfoText>{movie.imdbRating}</S.InfoText>\n              </S.RowWrapper>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>{movie.Plot}</S.InfoText>\n            </S.SpaceWrapper>\n            <S.SpaceWrapper>\n              <S.InfoText>Director: {movie.Director}</S.InfoText>\n              <S.InfoText>Release: {movie.Released}</S.InfoText>\n              <S.InfoText>Actors: {movie.Actors}</S.InfoText>\n            </S.SpaceWrapper>\n            {movie.trailer && (\n              <S.SpaceWrapper>\n                <S.ResponsiveIframe\n                  title=\"Titulo do Iframe\"\n                  src={`https://www.youtube.com/embed/${getYouTubeId(\n                    movie.trailer\n                  )}?autoplay=0&mute=1`}\n                  frameBorder=\"0\"\n                  allow=\"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\"\n                  allowFullScreen\n                />\n              </S.SpaceWrapper>\n            )}\n          </S.InfoWrapper>\n        </>\n      )}\n    </S.Container>\n  );\n}\n",["99","100"],"/home/adailsonaguiar/dev/infoplay/src/pages/DetailMovie/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Heading/styles.js",["101"],"/home/adailsonaguiar/dev/infoplay/src/components/Heading/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/pages/MainRoute.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/Footer/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/TextField/styles.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Shimmer/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/components/Navbar/index.jsx",[],"/home/adailsonaguiar/dev/infoplay/src/contexts/index.js",[],"/home/adailsonaguiar/dev/infoplay/src/components/Button/index.js",[